#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int n,t;
    scanf("%d %d ",&n , &t);
    
    int count=0;
    int sum=0;
    for(int i=1;i<=n;i++){
        int book;
        scanf("%d ",&book);
        if(sum<=t){
            sum+=book;
        }
        if(sum<=t){
            count++;
        }
    }
    printf("%d ",count);
    
    return 0;
}
